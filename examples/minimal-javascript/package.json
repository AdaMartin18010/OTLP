    "@opentelemetry/context-async-hooks": "^1.18.1",
    "@opentelemetry/propagator-b3": "^1.18.1",
    "@opentelemetry/propagator-jaeger": "^1.18.1",
    "express": "^4.18.2",
    "axios": "^1.6.2",
    "winston": "^3.11.0"
