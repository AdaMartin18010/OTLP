{
  "name": "minimal-javascript",
  "version": "1.0.0",
  "description": "Minimal OpenTelemetry JavaScript example with traces and metrics",
  "main": "main.js",
  "type": "module",
  "scripts": {
    "start": "node main.js",
    "benchmark": "node benchmark.js",
    "test": "node test.js",
    "dev": "node --watch main.js",
    "lint": "eslint .",
    "format": "prettier --write ."
  },
  "keywords": [
    "opentelemetry",
    "tracing",
    "metrics",
    "observability",
    "javascript",
    "nodejs"
  ],
  "author": "OpenTelemetry JavaScript Team",
  "license": "MIT",
  "dependencies": {
    "@opentelemetry/api": "^1.7.0",
    "@opentelemetry/sdk-node": "^0.45.1",
    "@opentelemetry/auto-instrumentations-node": "^0.40.3",
    "@opentelemetry/exporter-otlp-http": "^0.45.1",
    "@opentelemetry/exporter-otlp-grpc": "^0.45.1",
    "@opentelemetry/instrumentation": "^0.45.1",
    "@opentelemetry/instrumentation-http": "^0.45.1",
    "@opentelemetry/instrumentation-fs": "^0.10.0",
    "@opentelemetry/instrumentation-dns": "^0.30.0",
    "@opentelemetry/instrumentation-net": "^0.30.0",
    "@opentelemetry/instrumentation-express": "^0.32.0",
    "@opentelemetry/instrumentation-fetch": "^0.45.1",
    "@opentelemetry/resources": "^1.18.1",
